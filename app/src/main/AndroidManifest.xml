<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ib.healthapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthApp">
        <activity android:name=".RemindersSetterActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".MedicinesActivity" />
        <activity android:name=".AllRemindersActivity" />
        <activity android:name=".ResultInterpretation" />
        <activity android:name=".Results" />
        <activity android:name=".UserActivity" />
        <activity android:name=".NewUserActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>