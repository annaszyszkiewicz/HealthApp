<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ib.healthapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HealthApp">
        <activity android:name=".activity.RaportActivity"></activity>
        <activity
            android:name=".activity.PlotStatsActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.RemindersSetterActivity" />
        <activity android:name=".activity.HistoryStatsActivity" />
        <activity android:name=".activity.SettingsActivity" />
        <activity android:name=".activity.MedicinesActivity" />
        <activity android:name=".activity.AllRemindersActivity" />
        <activity android:name=".activity.ResultInterpretationActivity" />
        <activity android:name=".activity.ResultsActivity" />
        <activity android:name=".activity.UserActivity" />
        <activity android:name=".activity.NewUserActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>